<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='consulo'>

  <collapse-all-properties/>

  <inherits name='com.google.gwt.user.RemoteService'/>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <inherits name="com.github.gwtbootstrap.Bootstrap"/>
  <inherits name="org.cafesip.gwtcomp.GWTComponents"/>

  <stylesheet src="css/project.css"/>

  <public path="client/resource"/>

  <entry-point class='consulo.web.gwt.client.GwtMain'/>

  <source path='shared'/>
  <source path='client'/>

</module>
