<idea-plugin>
  <id>org.consulo.java.platform</id>
  <name>Java Platform</name>
  <version>1.0</version>
  <vendor>Consulo</vendor>

  <extensions defaultExtensionNs="com.intellij">
    <moduleExtensionProvider id="java" key="java" implementationClass="org.consulo.java.platform.module.extension.JavaModuleExtensionProvider" />

    <psi.packageSupportProvider implementation="org.consulo.java.platform.psi.impl.JavaPsiPackageSupportProvider" />

    <sdkType implementation="org.consulo.java.platform.module.sdk.JavaSdkType" />
  </extensions>
</idea-plugin>